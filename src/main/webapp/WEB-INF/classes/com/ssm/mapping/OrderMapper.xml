<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssm.mapper.OrderMapper">



    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ssm.entity.OrderUser">
        <id column="id" property="id" />
        <result column="gid" property="gid" />
        <result column="pice" property="pice" />
        <result column="sid" property="sid" />
        <result column="bid" property="bid" />
        <result column="adress" property="adress" />
        <result column="ipone" property="ipone" />
        <result column="creat_time" property="creatTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, gid, pice, sid, bid, adress, ipone, creat_time AS creatTime
    </sql>

</mapper>
