<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssm.mapper.GoodsMapper">



    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ssm.entity.Goods">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="pice" property="pice" />
        <result column="gurl" property="gurl" />
        <result column="content" property="content" />
        <result column="uid" property="uid" />
        <result column="count" property="count" />
        <result column="type" property="type" />
        <result column="aid" property="aid" />
        <result column="message" property="message" />
        <result column="creat_time" property="creatTime" />
        <result column="end_time" property="endTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, pice, gurl, content, uid, count, type, aid, message, creat_time AS creatTime, end_time AS endTime
    </sql>

</mapper>
